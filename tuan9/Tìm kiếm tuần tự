def linear_search(arr, target):
    for i in range(len(arr)):
        if arr[i] == target:
            return i
    return -1

#vi du
books = ["Toán", "Lý", "Hóa", "Văn", "Sử"]
print(f"Tìm sách 'Toán' tại vị trí: {linear_search(books, 'Toán')}")
print(f"Tìm sách 'Sinh' tại vị trí: {linear_search(books, 'Sinh')}")
